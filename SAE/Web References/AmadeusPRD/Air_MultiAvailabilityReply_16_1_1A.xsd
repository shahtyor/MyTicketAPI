<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://xml.amadeus.com/SATRSP_16_1_1A" elementFormDefault="qualified" targetNamespace="http://xml.amadeus.com/SATRSP_16_1_1A" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Air_MultiAvailabilityReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Availability/Schedule/Timetable Response Message</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="messageActionDetails" type="MessageActionDetailsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Specification of the action code of the response</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="5" name="errorOrWarningSection">
          <xs:annotation>
            <xs:documentation xml:lang="en">Warnings and Errors found at process level</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="errorOrWarningInfo" type="ApplicationErrorInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Error or Warning message found at process level</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="textInformation" type="InteractiveFreeTextTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Error or Warning free flow text found at process level</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="2" name="singleCityPairInfo">
          <xs:annotation>
            <xs:documentation xml:lang="en">Information related to one city pair</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="locationDetails" type="OriginAndDestinationDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Specification of board point and off point of the segment</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="9" name="cityPairFreeFlowText" type="InteractiveFreeTextTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Free flow text found at city pair level</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="5" name="cityPairErrorOrWarning">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Warnings and Errors found at city pair level</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="cityPairErrorOrWarningInfo" type="ApplicationErrorInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Error or Warning message found at city pair level</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="cityPairErrorOrWarningText" type="InteractiveFreeTextTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Error or Warning free flow text found at city pair level</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="300" name="flightInfo">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Information related to a flight. The repetition has been increased to 300 to support availability award calendar requests.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="basicFlightInfo" type="AvailabilityProductInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Main information on the flight</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="dateInfo" type="DateAndTimeInformationType" />
                    <xs:element minOccurs="0" name="pointTypeDetails" type="CodedAttributeType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Tells the type of the board/off point for this travel solution.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="providerDetails" type="CompanyInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">The provider details of this travel solution.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="26" name="infoOnClasses" type="ClassInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Information on classes</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="4" name="cabinClassInfo">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Information related to classes ordered by cabin</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="cabinInfo" type="CabinDetailsType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Information on cabins</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element minOccurs="0" maxOccurs="26" name="infoByCabinOnClasses" type="ClassInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Information on classes associated to a cabin</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="2" name="additionalFlightInfo" type="AdditionalProductDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Additional information on the flight</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="realTimeDateTimes" type="StructuredDateTimeInformationType" />
                    <xs:element minOccurs="0" maxOccurs="4" name="stops" type="ConnectionType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Lists the stops and indicates if border point</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="servicesOnboard" type="FreeTextInformationType" />
                    <xs:element minOccurs="0" maxOccurs="99" name="ssrQuotaDetails" type="SpecialRequirementsQuotaType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">SSR informations at leg/flight level</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="26" name="legCabinInfo">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Leg cabin informations</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="cabinIdentification" type="SegmentCabinIdentificationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Contains the cabin code</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="legCabinCapacity" type="AircraftConfigurationVersionType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Leg Cabin Capacity</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="legCabinAvlCounters" type="LegCabinAvailabilitiesType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Availability counters</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="trafficRestrictionList" type="TrafficRestrictionTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Specification of the traffic restrictions applied on a segment</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="aircraftInformation">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Group with ACV code and saleable config.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="acvInformation" type="AircraftConfigurationVersionType_167685S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Aircraft Configuration View information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="saleableConfiguration" type="EquipmentInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Saleable config information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="cabinClassServiceList" type="CabinClassServicesType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This segment handles list of Cabins/Classes informations</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="6" name="flightErrorWarningSection">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Warnings and Errors found at flight level</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="flightErrorWarningInfo" type="ApplicationErrorInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Error or Warning message found at flight level</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element minOccurs="0" name="flightErrorWarningText" type="InteractiveFreeTextTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Error or Warning free flow text found at flight level</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="negoSpaceDetails" type="NegociatedSpaceType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify information on a negociated space availability</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" name="travelChoiceMessage">
          <xs:annotation>
            <xs:documentation xml:lang="en">Customer Server Edifact message</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="summaryAndAvailabilityLines" type="PrefAndPolicySummaryAndAvailLineType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To summarise the preferences and policies that have been selected and the availability line</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element maxOccurs="3" name="preferencesSection">
                <xs:annotation>
                  <xs:documentation xml:lang="en">To specify air preferences</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="profileIdentificationSection" type="ReservationControlInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify a reference to a profile</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="travellerInformation" type="TravellerInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify a traveller and personal details relating to the traveller</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="associationName" type="ProfileNameType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify a profile name for corporate, agencies or organizations</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="originAndDestinationRules" type="OriginAndDestinationRequestType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify origin and destination rules</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="cabinRules" type="CabinEligibilityRulesType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify Cabin eligibility rules</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="availabilityInformationSection" type="AvailabilityInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify availability informations</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="itineraryPreference" type="ItineraryPreferencesType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify itinerary preferences</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AdditionalEquipmentInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify equipment configuration, registration number, and cabin facilities.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="code" type="AlphaNumericString_Length1To17">
        <xs:annotation>
          <xs:documentation xml:lang="en">Aircraft Configuration View information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalProductDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey additional information concerning an airline flight</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="flightDetails" type="AdditionalProductTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Additional details describing a specific means of transport</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="departureStation" type="StationInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Departure Station Information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="arrivalStation" type="StationInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Arrival Station Information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="milageDetails" type="MileageTimeDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Accumulated elapsed time for multi-leg flight and distance by flight leg.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="travellerTimeDetails" type="TravellerTimeDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Passenger departure and arrival times from gate (e.g. airports with mobile lounges)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="99" name="productFacilities" type="ProductFacilitiesTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Defines indicators for facilities</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalProductTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Additional product details</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="typeOfAircraft" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">UN/IATA code identifying type of aircraft</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="numberOfStops" type="NumericInteger_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of stops enroute made in a journey</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="legDuration" type="Duration99_HHMM">
        <xs:annotation>
          <xs:documentation xml:lang="en">Elapsed flying time of the journey (hhmm)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="onTimePercentage" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">On time performance indicator</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="dayOfOperation" type="NumericInteger_Length1To7">
        <xs:annotation>
          <xs:documentation xml:lang="en">Day of week indicator based on schedules format:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="effectivePeriod" type="AlphaNumericString_Length1To12">
        <xs:annotation>
          <xs:documentation xml:lang="en">Time table effective and discontinue date: ddmmyyddmmyy</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="complexingFlightIndicator" type="AlphaNumericString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicates if the flight is a complexing or a change of gauge flight</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="firstChangeOfGauge" type="AlphaString_Length3To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">3 character airport/city code where 1st change of gauge occurs</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="secondChangeOfGauge" type="AlphaString_Length3To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">3 character airport/city code where 2nd change of gauge occurs</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="thirdChangeOfGauge" type="AlphaString_Length3To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">3 character airport/city code where 3rd change of gauge occurs</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AircraftConfigurationVersionType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify aircraft configuration version of an aircraft</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="5" name="cabinClassDetails" type="ConfigurationDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">To Cabin/Class Identification</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AircraftConfigurationVersionType_167685S">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify aircraft configuration version of an aircraft</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="configVersionDetails" type="AdditionalEquipmentInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">ADDITIONAL EQUIPMENT INFORMATION Aircraft configuration version details</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorDetailTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Application error detail</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="code" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">The code assigned by the receiver of a message for identification of  a data validation error condition.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="type" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Identifies type of code list</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="listResponsible" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Identifies agency responsible for code list</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ApplicationErrorInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of application error within a message</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="error" type="ApplicationErrorDetailTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Defines the type and code of an error</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AssociatedPreferenceType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Preference associated</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="rcType" type="AlphaNumericString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Type of profile associated to this class:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="highlight" type="AlphaNumericString_Length2To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Level of highlighting :</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AssociatedPreferenceType_237163C">
    <xs:annotation>
      <xs:documentation xml:lang="en">Preference associated</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="rcType" type="AlphaNumericString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Type of profile associated to this company:</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="highlight" type="AlphaNumericString_Length2To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Level of highlighting</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="priority" type="NumericInteger_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Priority for airline preference</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AvailabilityComponentType">
    <xs:sequence>
      <xs:element minOccurs="0" name="reservationControlType" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Reservation control type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="informationLine" type="AlphaNumericString_Length1To54">
        <xs:annotation>
          <xs:documentation xml:lang="en">Availability information line</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="moreIndicator" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">More selected availability lines indicator, coded</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AvailabilityInformationDetailsType">
    <xs:sequence>
      <xs:element name="line" type="AlphaNumericString_Length1To54">
        <xs:annotation>
          <xs:documentation xml:lang="en">Availability information line</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="destinationValue" type="AlphaNumericString_Length2To5">
        <xs:annotation>
          <xs:documentation xml:lang="en">Destination value</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AvailabilityInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify Availability Information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="availabilityInformation" type="AvailabilityInformationDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">AVAILABILITY INFORMATION</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AvailabilityProductInformationType">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify details related to a product</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="flightDetails" type="ProductDateTimeType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information on date and time of a flight</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="departureLocation" type="LocationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Departure location of the flight</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="3" name="departurePreference" type="AssociatedPreferenceType">
        <xs:annotation>
          <xs:documentation xml:lang="en">To give a reference to a customer preference associated to this departure point.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="arrivalLocation" type="LocationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Arrival location of the flight</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="3" name="arrivalPreference" type="AssociatedPreferenceType">
        <xs:annotation>
          <xs:documentation xml:lang="en">To give a reference to a customer preference associated to this arrival point.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="marketingCompany" type="CompanyIdentificationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Defines the marketing airline of the flight</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="3" name="markCompanyPreferences" type="AssociatedPreferenceType_237163C">
        <xs:annotation>
          <xs:documentation xml:lang="en">To give a reference to a customer preference associated to this company</xs:documentation>
